{
  "expo": {
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification.png",
          "color": "#ffffff"
        }
      ]
    ],
    "name": "ihelp-magazine",
    "description": "ihelp magazine",
    "slug": "ihelp-magazine",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#041C32"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "mn.ihelp.novelist",
      "buildNumber": "1.0.0"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "versionCode": 1,
      "package": "mn.ihelp.novelist",
      "permissions": [],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-square.png",
        "backgroundColor": "#FFFFFF"
      }
    }
   
  }
}
